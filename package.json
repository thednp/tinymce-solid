{
  "name": "tinymce-solid",
  "version": "0.0.1",
  "description": "TinyMCE for SolidJS",
  "type": "module",
  "workspaces": [
    "demo/",
    "/"
  ],
  "homepage": "https://thednp.github.io/tinymce-solid",
  "bugs": {
    "url": "https://github.com/thednp/tinymce-solid/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"demo/**/*.{js,ts,json,css,tsx,jsx}\"",
    "build": "tsup",
    "build:all": "pnpm build && pnpm build:docs",
    "dev": "pnpm --filter tinymce-solid-demo dev",
    "build:docs": "pnpm --filter tinymce-solid-demo build:docs",
    "copy:tiny": "cp -a './node_modules/.pnpm/tinymce@7.1.1/node_modules/tinymce' './demo/public'",
    "serve:docs": "pnpm --filter tinymce-solid-demo start",
    "prepublishOnly": "pnpm up && npm update && pnpm format && pnpm lint && pnpm build",
    "lint": "pnpm lint:types && pnpm lint:code",
    "lint:code": "eslint 'src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:types": "tsc --noEmit src/**/* --jsx preserve",
    "fix:ts": "eslint 'src/**/*.{ts,tsx}' --fix",
    "start": "pnpm serve:docs",
    "version": "vinxi version"
  },
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.13.5",
    "@solidjs/start": "^1.0.1",
    "@types/node": "^20.14.0",
    "@types/prop-types": "^15.7.12",
    "prop-types": "^15.8.1",
    "solid-js": "^1.8.17",
    "tinymce": "^7.1.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "prettier": "^3.3.0",
    "tsup": "^8.0.2",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0",
    "vinxi": "^0.3.11",
    "vite-plugin-compression2": "^1.1.1",
    "vite-plugin-solid": "^2.10.2"
  },
  "packageManager": "pnpm@9.1.4",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {}
}
